{
  "expo": {
    "name": "LivDaily",
    "slug": "LivDaily",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIAccessibilityTraits": "button",
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized wellness content and improve your experience.",
        "NSLocationWhenInUseUsageDescription": "Location is used to provide location-based wellness recommendations.",
        "NSCameraUsageDescription": "Camera access is used for capturing wellness moments.",
        "NSPhotoLibraryUsageDescription": "Photo library access is used to save and share your wellness journey."
      },
      "accessibilityFeatures": {
        "voiceOver": true,
        "largerText": true,
        "boldText": true,
        "reduceMotion": true,
        "increaseContrast": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "natively",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://ay2em9m26g82xd73zxnjvyw7bv8knh59.app.specular.dev"
    }
  },
  "scheme": "LivDaily"
}